<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd http://java.sun.com/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="4.0">
  <display-name>WebApp</display-name>
  <welcome-file-list>
    <welcome-file>Home.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>Win1251Filter</filter-name>
    <filter-class>com.TSU.Win1251Filter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>Win1251Filter</filter-name>
    <url-pattern>/Win1251Servlet</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>DayNightFilter</filter-name>
    <filter-class>com.TSU.DayNightFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>DayNightFilter</filter-name>
    <servlet-name>DayNightServlet</servlet-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>EnRuFilter</filter-name>
    <filter-class>com.TSU.EnRuFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>EnRuFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>JSPNameServlet</servlet-name>
    <servlet-class>com.TSU.JSPNameServlet</servlet-class>
  </servlet>
  <resource-env-ref>
    <resource-env-ref-name>jdbc/db</resource-env-ref-name>
    <resource-env-ref-type>javax.sql.DataSource</resource-env-ref-type>
  </resource-env-ref>
</web-app>